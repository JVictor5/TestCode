{
  "name": "@project/functions",
  "private": true,
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tsc --noEmit && eslint --ext .js,.ts .",
    "serve": "npm run build && firebase emulators:start --only functions --inspect-functions",
    "build": "npm run clean && npx swc src --copy-files -d lib",
    "build:watch": "npm run clean && npx swc src -w --copy-files -d lib",
    "deploy": "firebase deploy"
  },
  "main": "lib/index.js",
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@burand/functions": "^3.0.0",
    "busboy": "^1.6.0",
    "express": "^4.0.0",
    "file-type": "^19.0.0",
    "firebase-admin": "^11.0.0",
    "firebase-functions": "^4.0.0",
    "got": "^13.0.0",
    "reflect-metadata": "~0.1.0",
    "tsyringe": "^4.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@burand/eslint-config-functions": "^3.0.0",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.102",
    "@types/busboy": "^1.5.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  }
}
